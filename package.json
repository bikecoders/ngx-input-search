{
  "name": "input-search-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy-gh-pages": "npm run build:prod -- --base-href 'https://bikecoders.github.io/ngx-input-search/' && angular-cli-ghpages --repo=https://GH_TOKEN@github.com/bikecoders/ngx-input-search.git --dir=dist/input-search-demo --branch=gh-pages --name='bikecoders' --email='bikecoders@gmail.com'",
    "input-search:build": "ng build input-search",
    "input-search:build-prod": "ng build input-search --prod",
    "input-search:lint": "ng lint input-search",
    "input-search:lint:report": "npm --silent run input-search:lint -- --format json | sed -n '1!p' > reports/lint_issues.json",
    "input-search:publish": "npm run input-search:build-prod && npm run input-search:publish:copy-files && cd dist/input-search && npm publish",
    "input-search:publish:copy-licence": "copy LICENSE dist/input-search",
    "input-search:publish:copy-readme": "copy README.md dist/input-search",
    "input-search:publish:copy-files": "npm run input-search:publish:copy-licence && npm run input-search:publish:copy-readme",
    "input-search:test": "ng test input-search",
    "input-search:test:coverage": "ng test input-search -- --watch=false --code-coverage --browsers=ChromeHeadless",
    "input-search:test:sonar": "npm run input-search:lint:report && cross-env SONAR_QUBE=1 npm run input-search:test:coverage && sonar-scanner",
    "input-search:create-version": "cd projects/input-search && mkdir -p .git && npm version",
    "input-search:create-version:patch": "npm run input-search:create-version -- patch",
    "input-search:create-version:minor": "npm run input-search:create-version -- minor",
    "input-search:create-version:major": "npm run input-search:create-version -- major"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@ngx-loading-bar/http-client": "^2.2.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "ngx-input-search": "latest",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.5.0",
    "copy": "^0.3.2",
    "cross-env": "^5.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sonarqube-unit-reporter": "^0.0.18",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}