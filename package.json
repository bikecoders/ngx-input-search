    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "colors": "^1.3.3",
    "cross-env": "^5.2.0",
    "cypress": "3.4.0",
    "dotenv": "6.2.0",
    "eslint": "6.1.0",
    "husky": "^3.0.3",
    "jest": "24.1.0",
    "jest-preset-angular": "7.0.0",
    "ng-packagr": "^5.1.0",
    "prettier": "1.16.4",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
